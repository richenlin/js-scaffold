#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/Workspace/nodejs/js-scaffold/node_modules/.pnpm/@commitlint+cli@18.6.1_@types+node@22.13.10_typescript@5.8.2/node_modules/@commitlint/cli/node_modules:/mnt/d/Workspace/nodejs/js-scaffold/node_modules/.pnpm/@commitlint+cli@18.6.1_@types+node@22.13.10_typescript@5.8.2/node_modules/@commitlint/node_modules:/mnt/d/Workspace/nodejs/js-scaffold/node_modules/.pnpm/@commitlint+cli@18.6.1_@types+node@22.13.10_typescript@5.8.2/node_modules:/mnt/d/Workspace/nodejs/js-scaffold/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/Workspace/nodejs/js-scaffold/node_modules/.pnpm/@commitlint+cli@18.6.1_@types+node@22.13.10_typescript@5.8.2/node_modules/@commitlint/cli/node_modules:/mnt/d/Workspace/nodejs/js-scaffold/node_modules/.pnpm/@commitlint+cli@18.6.1_@types+node@22.13.10_typescript@5.8.2/node_modules/@commitlint/node_modules:/mnt/d/Workspace/nodejs/js-scaffold/node_modules/.pnpm/@commitlint+cli@18.6.1_@types+node@22.13.10_typescript@5.8.2/node_modules:/mnt/d/Workspace/nodejs/js-scaffold/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/@commitlint+cli@18.6.1_@types+node@22.13.10_typescript@5.8.2/node_modules/@commitlint/cli/cli.js" "$@"
else
  exec node  "$basedir/../.pnpm/@commitlint+cli@18.6.1_@types+node@22.13.10_typescript@5.8.2/node_modules/@commitlint/cli/cli.js" "$@"
fi
