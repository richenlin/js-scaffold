#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/Workspace/nodejs/js-scaffold/node_modules/.pnpm/standard-version@9.5.0/node_modules/standard-version/bin/node_modules:/mnt/d/Workspace/nodejs/js-scaffold/node_modules/.pnpm/standard-version@9.5.0/node_modules/standard-version/node_modules:/mnt/d/Workspace/nodejs/js-scaffold/node_modules/.pnpm/standard-version@9.5.0/node_modules:/mnt/d/Workspace/nodejs/js-scaffold/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/Workspace/nodejs/js-scaffold/node_modules/.pnpm/standard-version@9.5.0/node_modules/standard-version/bin/node_modules:/mnt/d/Workspace/nodejs/js-scaffold/node_modules/.pnpm/standard-version@9.5.0/node_modules/standard-version/node_modules:/mnt/d/Workspace/nodejs/js-scaffold/node_modules/.pnpm/standard-version@9.5.0/node_modules:/mnt/d/Workspace/nodejs/js-scaffold/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/standard-version@9.5.0/node_modules/standard-version/bin/cli.js" "$@"
else
  exec node  "$basedir/../.pnpm/standard-version@9.5.0/node_modules/standard-version/bin/cli.js" "$@"
fi
