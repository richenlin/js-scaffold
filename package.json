{
  "name": "your-project",
  "version": "1.0.0",
  "keywords": [],
  "author": "",
  "license": "MIT",
  "description": "",
  "type": "module",
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.cjs",
      "import": "./dist/esm/index.js"
    }
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "rm -rf dist",
    "build:cjs": "babel src --extensions .js --out-dir dist/cjs --config-file ./babel-cjs.config.json",
    "build:esm": "babel src --extensions .js --out-dir dist/esm --config-file ./babel-esm.config.json",
    "build": "npm run build:cjs && npm run build:esm",
    "docs": "npm run docs:api && npm run docs:lint",
    "docs:api": "documentation build src/** -f html -o docs/api --config documentation.yml",
    "docs:lint": "documentation lint src/**",
    "lint": "eslint --ext .js src",
    "lint:fix": "npm run lint -- --fix",
    "prerelease": "npm run lint && npm run docs && npm run build && npm run docs",
    "release": "standard-version",
    "release:alpha": "standard-version --prerelease alpha",
    "release:beta": "standard-version --prerelease beta",
    "release:rc": "standard-version --prerelease rc",
    "prepublishOnly": "npm test && npm run build",
    "prepare": "husky install",
    "version": "npm run build && git add dist"
  },
  "devDependencies": {
    "@babel/cli": "^7.26.4",
    "@babel/core": "^7.26.9",
    "@babel/preset-env": "^7.26.9",
    "@commitlint/cli": "^18.4.2",
    "@commitlint/config-conventional": "^18.4.2",
    "@openapitools/openapi-generator-cli": "^2.17.0",
    "better-docs": "^2.7.3",
    "cpx": "^1.2.1",
    "docdash": "^2.0.2",
    "documentation": "^14.0.3",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "husky": "^8.0.3",
    "jsdoc": "^4.0.4",
    "lint-staged": "^15.4.3",
    "prettier": "^3.5.3",
    "standard-version": "^9.5.0",
    "typedoc": "^0.27.9"
  }
}
